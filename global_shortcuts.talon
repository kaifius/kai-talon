find it: key(cmd-f)
save it: key(cmd-s)
close it: key(cmd-w)
quit it: key(cmd-q)

copy it: key(cmd-c)
cut it: key(cmd-x)
paste it: key(cmd-v)
undo it: key(cmd-z)
redo it: key(cmd-y)

rightest: key(cmd-right)
leftest: key(cmd-left)
downest: key(cmd-down)
uppest: key(cmd-up)

tab next: key(cmd-shift-])
tab last: key(cmd-shift-[)

app search: key(cmd-t)
app quit: key(cmd-q)

row search: key(cmd-alt-l)
row: key(cmd-shift-space)

spotlight: key(cmd-space)
oh: key(0)
emoji: key(cmd-ctrl-space)
